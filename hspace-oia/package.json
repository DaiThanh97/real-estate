    "@types/hapi__vision": "^5.5.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.10.1",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "code": "^5.2.4",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.9.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.13",
    "lab": "^18.0.2",
    "lint-staged": "^10.3.0",
    "mockdate": "^3.0.2",
