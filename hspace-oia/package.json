{
  "name": "halato",
  "version": "1.0.0",
  "description": "Halato backend project",
  "main": "index.js",
  "scripts": {
    "dev": "npm run nodemon:build",
    "dev:swagger": "SWAGGER_FILE_GENERATION=true npm run nodemon:build",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "nodemon:build": "nodemon --exec ./node_modules/.bin/ts-node -- src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "cmd": "node --require ts-node/register cli.js",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "./node_modules/.bin/eslint . --ext .ts,.tsx --fix",
    "test": "jest --coverage --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll --verbose true",
    "migrate": "npm run typeorm migration:run",
    "revert": "npm run typeorm migration:revert"
  },
  "private": true,
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.12"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "@halato/user": "^1.0.9",
    "@hapi/boom": "^9.1.1",
    "@hapi/bossy": "^5.0.1",
    "@hapi/hapi": "^20.0.3",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/mime-db": "^1.43.0",
    "@types/nunjucks": "^3.1.3",
    "@types/socket.io": "^2.1.12",
    "@types/underscore": "^1.10.24",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "aws-sdk": "^2.765.0",
    "bcrypt": "^5.0.0",
    "casbin": "^5.4.2",
    "chalk": "^4.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "core-js": "^3.6.5",
    "firebase-admin": "^9.4.2",
    "hapi-auth-jwt2": "^10.2.0",
    "hapi-pulse": "^3.0.0",
    "hapi-swagger": "^14.0.0",
    "hapijs-status-monitor": "^1.0.1",
    "joi": "^17.3.0",
    "js-base64": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.2",
    "pg": "^8.5.0",
    "reflect-metadata": "^0.1.13",
    "snyk": "^1.388.0",
    "socket.io": "^2.3.0",
    "tslib": "^2.1.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.32",
    "typeorm-adapter": "^1.2.0",
    "underscore": "^1.11.0",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-logzio": "^5.1.2",
    "yamljs": "^0.3.0",
    "yargonaut": "^1.1.4",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@hapi/code": "^8.0.2",
    "@hapi/lab": "^23.0.0",
    "@types/async": "^3.2.3",
    "@types/code": "^4.0.5",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.2",
    "@types/faker": "^5.1.0",
    "@types/hapi__hapi": "^20.0.3",
